{
  "name": "@jsenv/server",
  "version": "16.0.0",
  "description": "A modern Node.js HTTP server with declarative routing, content negotiation, and WebSocket support",
  "license": "MIT",
  "author": {
    "name": "dmail",
    "email": "dmaillard06@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/core",
    "directory": "packages/independent/backend/server"
  },
  "engines": {
    "node": ">=22.13.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "development": "./src/main.js",
        "default": "./dist/jsenv_server.js"
      }
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/dist/",
    "/src/"
  ],
  "scripts": {
    "build": "node --experimental-import-meta-resolve ./scripts/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "string-width": "7.2.0",
    "@jsenv/router": "0.0.8",
    "launch-editor": "2.10.0",
    "@jsenv/abort": "4.3.0",
    "@jsenv/humanize": "1.2.8",
    "@jsenv/url-meta": "8.5.3",
    "@jsenv/utils": "2.1.3",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "eventsource": "3.0.2",
    "undici": "7.2.3",
    "ws": "8.18.0",
    "@jsenv/abort": "4.3.0",
    "formidable": "3.5.2",
    "@jsenv/humanize": "1.2.8",
    "@jsenv/url-meta": "8.5.3",
    "@jsenv/utils": "2.1.2"
  }
}
