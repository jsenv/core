{
  "name": "@jsenv/test",
  "version": "3.7.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/core",
    "directory": "packages/related/test"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "development": "./src/main.js",
        "default": "./dist/jsenv_test.js"
      }
    },
    "./*": "./*"
  },
  "files": [
    "/dist/",
    "/src/"
  ],
  "sideEffects": [
    "./src/runtime_node/node_child_process_controlled.mjs",
    "./src/runtime_node/node_worker_thread_controlled.mjs",
    "./dist/js/node_child_process_controlled.mjs",
    "./dist/js/node_worker_thread_controlled.mjs",
    "./dist/jsenv_test_node_modules.js",
    "./dist/jsenv_test.js"
  ],
  "scripts": {
    "build": "node --conditions=development ./scripts/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "optionalDependencies": {
    "playwright": "1.x",
    "playwright-core": "1.x"
  },
  "dependencies": {
    "@c88/v8-coverage": "0.1.1",
    "@jsenv/ast": "6.6.10",
    "@jsenv/plugin-supervisor": "1.6.14",
    "@jsenv/sourcemap": "1.3.6",
    "he": "1.2.0",
    "istanbul-lib-coverage": "3.2.2",
    "istanbul-lib-instrument": "6.0.3",
    "istanbul-lib-report": "3.0.1",
    "istanbul-reports": "3.1.7",
    "pidtree": "0.6.0",
    "v8-to-istanbul": "9.3.0"
  },
  "devDependencies": {
    "@jsenv/abort": "4.3.1",
    "@jsenv/exception": "1.1.8",
    "@jsenv/filesystem": "4.15.1",
    "@jsenv/github-check-run": "0.3.0",
    "@jsenv/node-esm-resolution": "1.2.0",
    "@jsenv/url-meta": "8.5.7",
    "@jsenv/urls": "2.7.3",
    "@jsenv/utils": "2.3.1",
    "@jsenv/humanize": "1.5.1",
    "strip-ansi": "7.1.0",
    "supports-color": "10.0.0"
  }
}
