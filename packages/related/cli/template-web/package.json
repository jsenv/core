{
  "name": "jsenv-template-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node --watch ./scripts/dev.mjs",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "build": "node ./scripts/build.mjs",
    "build:serve": "node ./scripts/build_serve.mjs --open",
    "eslint": "npx eslint .",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "7.27.1",
    "@jsenv/assert": "4.5.4",
    "@jsenv/core": "40.10.0",
    "@jsenv/eslint-config-relax": "1.7.1",
    "@jsenv/plugin-bundling": "2.10.0",
    "@jsenv/plugin-minification": "1.7.2",
    "@jsenv/test": "3.7.12",
    "eslint": "9.38.0",
    "open": "10.2.0",
    "@playwright/browser-chromium": "1.56.1",
    "prettier": "3.6.2",
    "playwright": "1.56.1"
  }
}
