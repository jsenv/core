{
  "name": "jsenv-template-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node --watch ./scripts/dev.mjs",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "build": "node ./scripts/build.mjs",
    "build:serve": "node ./scripts/build_serve.mjs --open",
    "eslint": "npx eslint .",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "7.26.0",
    "@jsenv/assert": "4.4.6",
    "@jsenv/core": "40.3.0",
    "@jsenv/eslint-config-relax": "1.4.7",
    "@jsenv/plugin-bundling": "2.9.0",
    "@jsenv/plugin-minification": "1.6.2",
    "@jsenv/test": "3.6.12",
    "eslint": "9.22.0",
    "open": "10.1.0",
    "@playwright/browser-chromium": "1.51.0",
    "prettier": "3.5.3"
  }
}
