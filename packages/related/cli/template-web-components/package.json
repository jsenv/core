{
  "name": "jsenv-template-web-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node --watch ./scripts/dev.mjs",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "build": "node ./scripts/build.mjs",
    "build:serve": "node ./scripts/build_serve.mjs --open",
    "eslint": "npx eslint .",
    "prettier": "prettier --write .",
    "playwright:install": "npx playwright install-deps && npx playwright install"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "7.28.6",
    "@jsenv/custom-elements-redefine": "0.0.1",
    "@jsenv/assert": "4.5.5",
    "@jsenv/core": "40.12.7",
    "@jsenv/plugin-bundling": "2.10.7",
    "@jsenv/plugin-minification": "1.7.3",
    "@jsenv/eslint-config-relax": "1.7.14",
    "@jsenv/test": "3.7.18",
    "eslint": "9.39.2",
    "open": "11.0.0",
    "@playwright/browser-chromium": "1.58.1",
    "prettier": "3.8.1",
    "playwright": "1.58.1"
  }
}
