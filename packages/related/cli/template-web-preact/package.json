{
  "name": "jsenv-template-web-preact",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node --watch ./scripts/dev.mjs",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "build": "node ./scripts/build.mjs",
    "build:serve": "node ./scripts/build_serve.mjs --open",
    "eslint": "npx eslint .",
    "prettier": "prettier --write .",
    "playwright:install": "npx playwright install-deps && npx playwright install"
  },
  "dependencies": {
    "preact": "10.26.4"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "7.26.0",
    "@babel/plugin-transform-react-jsx": "7.25.9",
    "@jsenv/assert": "4.4.3",
    "@jsenv/core": "40.0.0",
    "@jsenv/plugin-preact": "1.7.0",
    "@jsenv/plugin-bundling": "2.8.0",
    "@jsenv/plugin-minification": "1.6.0",
    "@jsenv/eslint-config-relax": "1.4.0",
    "@jsenv/test": "3.6.0",
    "eslint": "9.18.0",
    "open": "10.1.0",
    "@playwright/browser-chromium": "1.49.1",
    "prettier": "3.4.2"
  }
}
