      }
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/src/",
    "/dist/"
  ],
  "dependencies": {
    "@jsenv/ast": "6.0.6",
    "@jsenv/urls": "2.2.6",
    "@jsenv/sourcemap": "1.2.9",
    "launch-editor": "2.8.0"
  },
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs,.html",
    "build": "node ./scripts/build.mjs",
    "test": "node ./scripts/test.mjs",
    "prepublishOnly": "npm run build"
  }
}
