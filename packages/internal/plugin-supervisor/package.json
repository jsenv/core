    "/src/",
    "/dist/"
  ],
  "sideEffects": [
    "./src/client/supervisor.js"
  ],
  "dependencies": {
    "@jsenv/ast": "6.6.1",
    "@jsenv/urls": "2.6.2",
    "@jsenv/sourcemap": "1.3.1"
  },
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "prepublishOnly": "npm run build"
  }
}
