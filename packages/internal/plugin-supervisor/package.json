{
  "name": "@jsenv/plugin-supervisor",
  "version": "1.6.8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/core",
    "directory": "packages/internal/plugin-supervisor"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
