{
  "name": "@jsenv/plugin-supervisor",
  "version": "1.6.17",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/core",
    "directory": "packages/internal/plugin-supervisor"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "dev:jsenv": "./src/main.js",
        "default": "./dist/jsenv_plugin_supervisor.js"
      }
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/src/",
    "/dist/"
  ],
  "sideEffects": [
    "./src/client/supervisor.js",
    "./dist/js/supervisor.js"
  ],
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@jsenv/ast": "6.7.1",
    "@jsenv/sourcemap": "1.3.7"
  },
  "devDependencies": {
    "@jsenv/urls": "../../independent/tooling/urls",
    "@jsenv/snapshot": "../../independent/tooling/snapshot",
    "@jsenv/filesystem": "../../independent/backend/filesystem",
    "@jsenv/humanize": "../../independent/tooling/humanize",
    "@jsenv/core": "../../../"
  }
}
