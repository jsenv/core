    "@jsenv/chromium-launcher": "3.15.0",
    "@jsenv/codecov-upload": "2.0.0",
    "@jsenv/eslint-config": "11.3.0",
    "@jsenv/execution": "6.11.0",
    "@jsenv/node-launcher": "4.26.0",
    "@jsenv/prettier-check-project": "3.5.0",
