{
  "name": "@jsenv/core",
  "version": "24.2.1",
  "description": "Tool to develop, test and build js projects",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-core"
  },
  "engines": {
    "node": ">=14.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./main.js"
    },
    "./*": "./*"
  },
  "main": "main.js",
  "files": [
    "/dist/",
    "/helpers/",
    "/src/",
    "/main.js"
  ],
  "scripts": {
    "eslint-check": "node ./node_modules/eslint/bin/eslint.js . --ext=.js,.mjs,.cjs,.html",
    "importmap-generate": "node ./script/importmap/generate_importmap.js",
    "build": "node ./script/build/build.js",
    "dist": "npm run build",
    "test": "node --experimental-import-meta-resolve ./script/test/test.js",
    "dev": "node ./script/dev/start_dev_server.js",
    "test-with-coverage": "npm run test -- --coverage",
    "test-js-transform": "node ./script/test/test-js-transform.js",
    "test-compile-server": "node ./script/test/test-compile-server.js",
    "test-execute-test-plan": "node ./script/test/test-execute-test-plan.js",
    "test-build": "node ./script/test/test_build.js",
    "test-launch-browser": "node ./script/test/test-launch-browser.js",
    "test-launch-node": "node ./script/test/test-launch-node.js",
    "measure-performances": "node --expose-gc ./script/performance/generate_performance_report.js --log --once",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "playwright-install": "npx playwright install-deps && npx playwright install",
    "packages-install": "node ./script/packages/packages_install.js",
    "postinstall": "npm run importmap-generate && npm run playwright-install && npm run packages-install",
    "prepublishOnly": "node ./script/publish/remove_postinstall.mjs && npm run dist",
    "postpublish": "node ./script/publish/restore_postinstall.mjs"
  },
  "optionalDependencies": {
    "playwright": "1.x"
  },
  "dependencies": {
    "@babel/core": "7.16.0",
    "@babel/helper-module-imports": "7.16.0",
    "@babel/helpers": "7.16.3",
    "@babel/parser": "7.16.4",
    "@babel/plugin-proposal-dynamic-import": "7.16.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-assertions": "7.16.0",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-syntax-numeric-separator": "7.10.4",
    "@babel/plugin-transform-modules-systemjs": "7.16.0",
    "@c88/v8-coverage": "0.1.1",
    "@jsenv/abort": "4.1.1",
    "@jsenv/filesystem": "2.5.1",
    "@jsenv/importmap": "1.2.0",
    "@jsenv/log": "1.4.0",
    "@jsenv/logger": "4.0.1",
    "@jsenv/server": "12.2.0",
    "@jsenv/uneval": "1.6.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-replace": "3.0.0",
    "acorn-import-assertions": "1.8.0",
    "ansi-to-html": "0.7.2",
    "bytes": "3.1.1",
    "cjs-module-lexer": "1.2.2",
    "construct-style-sheets-polyfill": "3.0.4",
    "cssnano": "5.0.11",
    "cssnano-preset-default": "5.1.7",
    "cuid": "2.1.8",
    "estree-walker": "3.0.0",
    "html-minifier": "4.0.0",
    "humanize-duration": "3.27.0",
    "is-valid-identifier": "2.0.2",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-instrument": "5.1.0",
    "istanbul-lib-report": "3.0.0",
    "istanbul-reports": "3.0.5",
    "magic-string": "0.25.7",
    "parse5": "6.0.1",
    "pidtree": "0.5.0",
    "postcss": "8.3.11",
    "postcss-value-parser": "4.1.0",
    "regenerator-runtime": "0.13.9",
    "resolve": "1.20.0",
    "rollup": "2.60.1",
    "rollup-plugin-node-builtins-brofs": "2.1.3",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-polyfill-node": "0.7.0",
    "source-map": "0.7.3",
    "systemjs": "6.11.0",
    "terser": "5.10.0",
    "v8-to-istanbul": "8.1.0",
    "vm2": "3.9.5",
    "wrap-ansi": "8.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.3",
    "@babel/plugin-syntax-jsx": "7.16.0",
    "@babel/plugin-transform-block-scoping": "7.16.0",
    "@babel/plugin-transform-react-jsx": "7.16.0",
    "@babel/plugin-transform-typescript": "7.16.1",
    "@babel/preset-env": "7.16.4",
    "@jsenv/assert": "2.3.2",
    "@jsenv/babel-preset": "./packages/jsenv-babel-preset",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/github-release-package": "1.2.3",
    "@jsenv/https-local": "1.0.3",
    "@jsenv/importmap-eslint-resolver": "5.2.1",
    "@jsenv/importmap-node-module": "2.8.0",
    "@jsenv/package-publish": "1.6.2",
    "@jsenv/performance-impact": "2.2.1",
    "@jsenv/pwa": "4.0.0",
    "babel-plugin-transform-async-to-promises": "0.8.16",
    "eslint": "7.32.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-react": "7.27.1",
    "node-notifier": "10.0.0",
    "playwright": "1.16.3",
    "preact": "10.5.15",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "redux": "4.1.2",
    "rollup-plugin-import-assert": "1.1.1"
  }
}
