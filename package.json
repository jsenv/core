{
  "name": "@jsenv/core",
  "version": "11.2.2",
  "description": "Execute JavaScript on multiple environments for testing.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-core"
  },
  "engines": {
    "node": ">=12.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/commonjs/main.cjs"
    }
  },
  "module": "index.js",
  "main": "dist/commonjs/main.cjs",
  "files": [
    "/dist/",
    "/helpers/",
    "/src/",
    "/index.js"
  ],
  "scripts": {
    "start-exploring": "node ./script/start-exploring/start-exploring.js",
    "test": "node ./script/test/test.js",
    "test-with-coverage": "node ./script/test/test.js --coverage",
    "test-js-transform": "node ./script/test/test-js-transform.js",
    "test-compile-server": "node ./script/test/test-compile-server.js",
    "test-exploring": "node ./script/test/test-exploring.js",
    "test-commonjs-bundling": "node ./script/test/test-commonjs-bundling.js",
    "test-global-bundling": "node ./script/test/test-global-bundling.js",
    "test-systemjs-bundling": "node ./script/test/test-systemjs-bundling.js",
    "test-launch-chromium": "node ./script/test/test-launch-chromium.js",
    "test-launch-node": "node ./script/test/test-launch-node.js",
    "eslint-check": "eslint .",
    "prettier-format": "node ./script/prettier-format/prettier-format.js",
    "prettier-format-stage": "npm run prettier-format -- --staged",
    "prettier-check": "npm run prettier-format -- --dry-run",
    "upload-coverage": "node ./script/upload-coverage/upload-coverage.js",
    "generate-commonjs-bundle": "node ./script/generate-commonjs-bundle/generate-commonjs-bundle.js",
    "generate-import-map": "node ./script/generate-import-map/generate-import-map.js",
    "install-git-hooks": "node ./script/install-git-hooks/install-git-hooks.js",
    "dist": "npm run clean && npm run generate-commonjs-bundle",
    "clean": "rimraf dist && rimraf coverage",
    "reinstall": "npm run clean && rimraf node_modules && npm install",
    "postinstall": "npm run generate-import-map && npm run install-git-hooks",
    "prepublishOnly": "node ./script/transform-package/remove-postinstall.js && npm run dist",
    "postpublish": "node ./script/transform-package/restore-postinstall.js",
    "git-hook-pre-commit": "npm run prettier-format-stage"
  },
  "dependencies": {
    "@babel/core": "7.8.3",
    "@babel/helper-hoist-variables": "7.8.3",
    "@babel/helper-module-imports": "7.8.3",
    "@babel/helper-plugin-utils": "7.8.3",
    "@babel/helpers": "7.8.3",
    "@babel/plugin-proposal-json-strings": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-proposal-optional-catch-binding": "7.8.3",
    "@babel/plugin-proposal-unicode-property-regex": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "7.8.3",
    "@babel/plugin-syntax-optional-catch-binding": "7.8.3",
    "@babel/plugin-transform-arrow-functions": "7.8.3",
    "@babel/plugin-transform-block-scoped-functions": "7.8.3",
    "@babel/plugin-transform-block-scoping": "7.8.3",
    "@babel/plugin-transform-classes": "7.8.3",
    "@babel/plugin-transform-computed-properties": "7.8.3",
    "@babel/plugin-transform-destructuring": "7.8.3",
    "@babel/plugin-transform-dotall-regex": "7.8.3",
    "@babel/plugin-transform-duplicate-keys": "7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "7.8.3",
    "@babel/plugin-transform-for-of": "7.8.3",
    "@babel/plugin-transform-function-name": "7.8.3",
    "@babel/plugin-transform-literals": "7.8.3",
    "@babel/plugin-transform-modules-systemjs": "7.8.3",
    "@babel/plugin-transform-new-target": "7.8.3",
    "@babel/plugin-transform-object-super": "7.8.3",
    "@babel/plugin-transform-parameters": "7.8.3",
    "@babel/plugin-transform-regenerator": "7.8.3",
    "@babel/plugin-transform-shorthand-properties": "7.8.3",
    "@babel/plugin-transform-spread": "7.8.3",
    "@babel/plugin-transform-sticky-regex": "7.8.3",
    "@babel/plugin-transform-template-literals": "7.8.3",
    "@babel/plugin-transform-typeof-symbol": "7.8.3",
    "@babel/plugin-transform-unicode-regex": "7.8.3",
    "@jsenv/cancellation": "1.4.0",
    "@jsenv/href": "1.2.0",
    "@jsenv/import-map": "6.7.1",
    "@jsenv/logger": "3.3.0",
    "@jsenv/node-module-import-map": "11.0.0",
    "@jsenv/node-signals": "1.2.1",
    "@jsenv/server": "1.5.3",
    "@jsenv/uneval": "1.3.0",
    "@jsenv/util": "3.3.4",
    "@rollup/plugin-json": "4.0.1",
    "@rollup/plugin-commonjs": "11.0.1",
    "@rollup/plugin-node-resolve": "7.0.0",
    "@rollup/plugin-replace": "2.3.0",
    "ansi-to-html": "0.6.13",
    "babel-plugin-transform-async-to-promises": "0.8.15",
    "babel-plugin-transform-commonjs": "1.1.6",
    "clean-css": "4.2.2",
    "cuid": "2.1.8",
    "eventsource": "1.0.7",
    "html-minifier": "4.0.0",
    "istanbul-lib-coverage": "3.0.0-alpha.0",
    "istanbul-lib-instrument": "4.0.0-alpha.0",
    "istanbul-lib-report": "3.0.0-alpha.0",
    "istanbul-reports": "3.0.0-alpha.1",
    "node-notifier": "6.0.0",
    "proper-lockfile": "4.1.1",
    "puppeteer": "2.1.0",
    "regenerator-runtime": "0.13.3",
    "rimraf": "3.0.1",
    "rollup": "1.30.1",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "source-map": "0.7.3",
    "systemjs": "6.1.9",
    "terser": "4.6.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "7.8.3",
    "@jsenv/assert": "1.2.2",
    "@jsenv/codecov-upload": "3.2.0",
    "@jsenv/core": "11.2.1-alpha.1",
    "@jsenv/eslint-config": "12.2.0",
    "@jsenv/git-hooks": "1.2.0",
    "@jsenv/github-release-package": "1.1.1",
    "@jsenv/package-publish": "1.4.1",
    "@jsenv/prettier-check-project": "5.4.0",
    "@jsenv/prettier-config": "1.2.0",
    "babel-eslint": "11.0.0-beta.0",
    "eslint": "6.8.0",
    "prettier": "1.19.1",
    "react": "16.12.0"
  }
}
