{
  "eslint.autoFixOnSave": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": false
  },
  "search.exclude": {
    "dist/**": true,
    ".dist/**": true,
    "coverage/**": true
  },
  "javascript.validate.enable": false,
  "npm.autoDetect": "on",
  "npm.enableScriptExplorer": true,
  "npm.scriptExplorerAction": "open"
}
